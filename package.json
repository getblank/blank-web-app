{
    "name": "blank-web-app",
    "version": "0.1.175-6",
    "description": "SPA for Blank",
    "main": "dev.js",
    "scripts": {
        "start": "node dev",
        "prepublishOnly": "rm -rf ./dist/* && rm -rf ./release/* && webpack",
        "postpublish": "rm -rf ./dist/* && rm -rf ./release/*",
        "test": "echo \"Error: no test specified\" && exit 1",
        "patch": "npm version patch && npm run push",
        "push": "git push origin master --follow-tags && npm publish",
        "next": "npm version prerelease && git push origin next --follow-tags && npm publish --tag next"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/getblank/blank-web-app.git"
    },
    "keywords": [
        "Blank",
        "react"
    ],
    "author": {
        "email": "info+blank@sclif.pro",
        "name": "Evgeniy Kuvshinov & Evgeniy Ivakha"
    },
    "homepage": "https://getblank.github.io/site/",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/getblank/blank-web-app/issues"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-decorators": "^7.0.0",
        "@babel/plugin-proposal-do-expressions": "^7.0.0",
        "@babel/plugin-proposal-export-default-from": "^7.0.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
        "@babel/plugin-proposal-function-bind": "^7.0.0",
        "@babel/plugin-proposal-function-sent": "^7.0.0",
        "@babel/plugin-proposal-json-strings": "^7.0.0",
        "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
        "@babel/plugin-proposal-numeric-separator": "^7.0.0",
        "@babel/plugin-proposal-optional-chaining": "^7.0.0",
        "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
        "@babel/plugin-proposal-throw-expressions": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-syntax-import-meta": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "@babel/runtime": "^7.0.0",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.0",
        "babel-plugin-dynamic-import-node": "^1.2.0",
        "clean-webpack-plugin": "^0.1.19",
        "css-loader": "^1.0.1",
        "eslint-plugin-react": "^7.9.1",
        "expose-loader": "^0.7.1",
        "express": "^4.13.4",
        "gulp": "latest",
        "http-proxy": "^1.13.3",
        "less": "^3.8.1",
        "minimist": "^1.2.0",
        "react-hot-loader": "^4.3.12",
        "serve-static": "^1.10.2",
        "style-loader": "^0.23.1",
        "webpack": "^4.7.0",
        "webpack-bundle-analyzer": "^2.11.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-middleware": "^5.2.1",
        "webpack-hot-middleware": "^2.13.1",
        "ws": "latest"
    },
    "dependencies": {
        "blank-web-sdk": "^0.0.34",
        "brace": "^0.8.0",
        "classnames": "latest",
        "create-react-class": "^15.6.2",
        "d3": "^3.5.16",
        "font-awesome": "latest",
        "handlebars": "latest",
        "handlebars-pluralize": "^1.0.2",
        "invariant": "latest",
        "moment": "2.22.2",
        "normalize.css": "latest",
        "nvd3": "^1.8.6",
        "prop-types": "^15.6.0",
        "react": "16.13.1",
        "react-ace": "^5.8.0",
        "react-dom": "16.13.1",
        "uuid": "3.4.0",
        "whatwg-fetch": "^1.0.0"
    },
    "babel": {
        "presets": [
            "@babel/preset-react",
            "@babel/preset-env"
        ],
        "comments": true,
        "plugins": [
            "@babel/plugin-proposal-function-bind",
            "@babel/plugin-syntax-dynamic-import",
            "@babel/plugin-syntax-import-meta",
            "@babel/plugin-proposal-class-properties",
            "@babel/plugin-proposal-json-strings",
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": true
                }
            ],
            "@babel/plugin-proposal-function-sent",
            "@babel/plugin-proposal-export-namespace-from",
            "@babel/plugin-proposal-numeric-separator",
            "@babel/plugin-proposal-throw-expressions",
            "@babel/plugin-proposal-export-default-from",
            "@babel/plugin-proposal-logical-assignment-operators",
            "@babel/plugin-proposal-optional-chaining",
            [
                "@babel/plugin-proposal-pipeline-operator",
                {
                    "proposal": "minimal"
                }
            ],
            "@babel/plugin-proposal-nullish-coalescing-operator",
            "@babel/plugin-proposal-do-expressions",
            [
                "@babel/plugin-transform-runtime",
                {
                    "regenerator": true
                }
            ]
        ]
    }
}
